{
    "containerDefinitions": [
        {
            "name": "aodn-dev-container",
            "image": "<IMAGE>",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": "<PORT>",
                    "hostPort": "<PORT>",
                    "protocol": "<PROTOCOL>",
                    "appProtocol": "<APP_PROTOCOL>"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "ELASTIC_URL",
                    "value": "<ELASTIC_URL>"
                },
                {
                    "name": "ELASTIC_KEY",
                    "value": "<ELASTIC_KEY>"
                },
                {
                    "name": "HOST",
                    "value": "<HOST>"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "<ENVIRONMENT>"
                },
                {
                    "name": "PORT",
                    "value": "<PORT>"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/aodn-dev1",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "family": "aodn-dev1",
    "executionRoleArn": "<EXECUTION_ROLE_ARN>",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}